{
  "expo": {
    "name": "Pictalk",
    "slug": "pictalk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./pictalk/assets/images/icon.png",
    "scheme": "pictalk",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./pictalk/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./pictalk/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./pictalk/assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    }
  }
} 